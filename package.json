{
	"name": "vscode-nuget-gallery",
	"displayName": "NuGet Gallery",
	"publisher": "patcx",
	"description": "NuGet Gallery Extension makes installing and uninstalling NuGet packages easier",
	"version": "0.0.3",
	"preview": true,
	"icon": "docs/images/icon.png",
	"engines": {
		"vscode": "^1.37.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/patcx/vscode-nuget-gallery"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.start",
			"title": "Open Gallery",
			"category": "NuGet"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"xmldom": "^0.1.27",
		"xpath": "^0.0.27"
	}
}